CC=gcc
SRC=$(wildcard *.c)
OBJ= main.o school.o
NAME=school
RM=rm -f

all:  main.o school.o
	$(CC) $(OBJ) -o $(NAME) 
main.o: main.c
	$(CC)    -c -o main.o main.c
school.o: school.c
	$(CC)    -c -o school.o school.c

clean:
	$(RM) *~ *.swp $(NAME)
oclean:
	$(RM) $(OBJ)
fclean: clean oclean

re: oclean main.o school.o all
